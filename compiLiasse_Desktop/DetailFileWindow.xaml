<Window x:Class="compiLiasse_Desktop.DetailFileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:compiLiasse_Desktop"
        mc:Ignorable="d"
        Title="DetailFileWindow" Height="253" Width="278">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto" />
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="Auto" />
		</Grid.ColumnDefinitions>
		<TextBox x:Name="FilePdfTxtBox" HorizontalAlignment="Left" Margin="77,46,0,0" Text="Fichier pdf" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
		<Button x:Name="btnFileModifOK" Content="Modifier" HorizontalAlignment="Left" Margin="77,189,0,119" Click="btnFileModifOK_Click" IsDefault="True"/>
		<Button x:Name="btnFileModifCancel" Content="Annuler" HorizontalAlignment="Left" Margin="152,189,0,0" VerticalAlignment="Top" Click="btnFileModifCancel_Click" IsCancel="True"/>
	</Grid>
</Window>
